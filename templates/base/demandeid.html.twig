{% extends 'base.html.twig' %}
{% block title %}
{{parent()}}Demande de carte d'identité
{% endblock %}
{% block body %}
<div class="container text-center">
    <h1 class="text-center text-primary mt-4 pt-4 display-1 fw-bold">
        Suivez votre demande</h1>
</div>
<div class="container text-center">

    <h2>Veuillez préciser la date de votre demande</h2>

    <div class="col-12 bg-white p-4 m-0 text-primary">{{form(form)}}</div>

</div><br>
<div class="container bg-primary">
    <div class="progress">
        <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">25%</div>
    </div>
</div>
</div>
{% endblock %}